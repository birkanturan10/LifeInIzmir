@model IEnumerable<TypeLayer.Ulasim.Vapur>

@{
	Layout = null;
}

<!DOCTYPE html>

<html>
<head>
	<meta name="viewport" content="width=device-width" />
	<title>Vapur İskeleleri</title>
	<!--Bootstrap Links-->
	<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">
	<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3" crossorigin="anonymous"></script>
	<!--ApiSheet.css-->
	<link href="~/style_sheets/apisheet.css" rel="stylesheet" />
	<!--jQuery-->
	<script src="~/lib/jquery/dist/jquery.min.js"></script>
</head>
<body>
	<!--Navbar-->
	<nav class="navbar navbar-expand-lg bg-body-tertiary">
		<div class="container-md  d-flex justify-content-center">
			<a class="navbar-brand">İzmir'de Ulaşım</a>
		</div>
	</nav>
	<!--Table-->
	<div class="container">
		<table class="table table-light">
			<thead>
				<tr>
					<th>ID</th>
					<th>İskele Adı</th>
					<th>Aktif Mi?</th>
					<th>Arabalı Vapur İskelesi Mi?</th>
				</tr>
			</thead>
			<tbody>
				@foreach (var item in Model)
				{
					<tr>
						<td>@item.IskeleId</td>
						<td>@item.Adi</td>
						<td>@(item.AktifMi ? "Evet" : "Hayır")</td>
						<td>@(item.ArabaliVapurIskelesiMi ? "Evet" : "Hayır")</td>
					</tr>
				}
			</tbody>
		</table>
	</div>
</body>
</html>



